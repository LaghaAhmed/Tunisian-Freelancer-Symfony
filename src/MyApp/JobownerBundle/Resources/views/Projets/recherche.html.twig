{% extends "JobownerBundle::layoutJobowner.html.twig" %}
{% block content %}
    <h1>Liste des projets en cours</h1>
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <div class="box-body table-responsive no-padding">
                        <h1>Projet</h1>



                        {% for voiture in modeles  %}

                            Titre : {{ voiture.titre }}</br>
                            Description : {{ voiture.description }}</br>
                            Username : {{ voiture.username }}
                            {{ socialButtons() }}

                        {% endfor %}

                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}