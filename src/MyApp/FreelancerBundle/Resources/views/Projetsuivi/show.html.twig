{% extends "FreelancerBundle::layout.html.twig" %}

{% block body -%}
    <h1>Projetsuivi</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Projetid</th>
                <td>{{ entity.projetid }}</td>
            </tr>
            <tr>
                <th>Jobowner</th>
                <td>{{ entity.jobowner }}</td>
            </tr>
            <tr>
                <th>Freelancer</th>
                <td>{{ entity.freelancer }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('projetsuivi') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('projetsuivi_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
